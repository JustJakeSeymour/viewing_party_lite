<h3>Discover Movies</h3>

<%= button_to "Find Top Rated Movies", user_movies_path(@user, q: 'top_rated'), method: :post, local: true %>

<div id='keyword-search'>
  <%= form_with url: "/users/#{@user.id}/movies?q=keyword", method: :post, local: true do |f| %>
    <%= f.text_field :keyword %>
    <%= f.submit "Find Movies"%>
  <% end %>
</div>